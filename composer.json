{
    "name": "visa4/logger",
    "description": "Logger library",
    "type": "library",
    "homepage": "https://github.com/visa4/logger",
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-log" : "^2.9.1",
        "zendframework/zend-stratigility" : "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.8"
    },
    "autoload": {
        "psr-4": {
            "MiddlewareLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiddlewareLoggerTest\\": "test/LoggerTest/"
        }
    }
}
